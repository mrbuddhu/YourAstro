    <path d="M200 300C200 300 150 250 100 300C50 350 100 400 150 450C200 500 250 450 300 400C350 350 300 300 250 250C200 200 200 300 200 300Z" fill="#FFB6C1"/>
    <path d="M200 500C200 500 150 450 100 500C50 550 100 600 150 650C200 700 250 650 300 600C350 550 300 500 250 450C200 400 200 500 200 500Z" fill="#FFB6C1"/>
    
